package com.man.hack;

import java.util.Arrays;
import java.util.Scanner;

public class CandidateCode {
	
	
	public static int mask(int n) {
		  int mask = 0;
		  for (int i = 1; i< n/i; i*= 10) {
		    int digit = n/i % 10;
		    mask |= 1 << digit;
		  }
		  return mask;
		}

	
	
	public static int dp(int set, int memo[], int[] a) {
		  if (set == 0) {
		    return memo[set] = 0;
		  }
		  if (memo[set] != -1)
		    return memo[set];
		 
		  int res = 0;
		  for (int num: a) {
		    int bitmask = mask(num);
		    if ((set | bitmask) == set)
		      res = Integer.max(dp(set ^ mask(num), memo, a) + num, res);
		  }
		  return memo[set] = res;
		}
		/*public static int dp(int set, int memo[], int[] a) {
		  if (set == 0) {
		    return memo[set] = 0;
		  }
		  if (memo[(int) set] != -1)
		    return memo[set];

		  int res = 0;
		  for (int num: a) {
		    int bitmask = mask(num);
		    if ((set | bitmask) == set)
		      res = Integer.max(dp(set ^ mask(num), memo, a) + num, res);
		    
		  }
		  return memo[set] = res;
		}*/
	public static int maxSum(int[] boxElement){
		Arrays.sort(boxElement);
		int[] memo=new int[1<<10];
		  for (int i = 0; i < 1 << 10; i++) 
			  memo[i] = -1;
		 
		  int res = 0;
		  for (int i = 0; i < 1 << 10; i++) {
		    res = Integer.max(res, dp(i, memo, boxElement));
		  }
		
		return res;
		
		
	}

